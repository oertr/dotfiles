[alias]
	pclone = clone --filter=blob:none
	tree = log --graph --oneline --all --abbrev-commit
[commit]
	gpgSign = true
[init]
	defaultBranch = main
[core]
	autocrlf = input
[pull]
	rebase = false
[include]
	path = ~/.gitconfig.local
[url "git://github.com/"]
	insteadOf = readonly:
[url "git@github.com:"]
	insteadOf = github:
